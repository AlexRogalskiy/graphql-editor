{
    "compilerOptions": {
      "sourceMap": false,
      "target": "es6",
      "jsx": "react",
      "module": "commonjs",
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "declaration": false,
      "removeComments": true,
      "noUnusedLocals": true,
      "lib": ["es6", "es7", "esnext", "dom"],
      "outDir": "./lib",
      "baseUrl": "./src",
      "rootDir": "../",
      "types": ["cypress"]
    },
    "exclude": ["dist", "lib", "node_modules"]
  }
  